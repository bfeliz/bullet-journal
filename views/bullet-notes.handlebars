<h1 class="collection">{{postName}}</h1>

<div class="row collection-row">
  <div class="col s12">
    {{#each subCat}}
    <h2 class="mini-collection" id="mini{{id}}">{{subName}}</h2>
    <div class="input-field col s12" id="area{{id}}">
      <textarea id="{{id}}" class="materialize-textarea collect-textarea">{{subValue}}</textarea>
      <label for="textarea{{id}}"></label>
      <br>
      <br>
      <br>
      <br>
      <br>
      <br>
      <br>
    </div>
    {{/each}}
  </div>
</div>
